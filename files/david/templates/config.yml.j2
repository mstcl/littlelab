address: "0.0.0.0"
port: "8080"
dir: "/data"
users:
{% for key, value in webdav_users.items() %}
  {{ key }}:
    password: "{{ value.password | password_hash('bcrypt') }}"
    subdir: "{{ value.subdir }}"
    permissions: "{{ value.permission }}"
{% endfor %}

log:
  production: false
  debug: false
  error: true
  create: false
  read: false
  update: false
  delete: false

cors:
  origin: "{{ expanded_url }}"
  credentials: true
