tcp:
  routers:
    slsktcp:
      entryPoints:
        - "slsktcp"
      rule: "HostSNI(`*`)"
      service: slsktcp
      tls:
        passthrough: true

  services:
    slsktcp:
      loadBalancer:
        proxyProtocol:
          version: 2
        servers:
          - address: "{{ proxy_target_ip }}:50300"
