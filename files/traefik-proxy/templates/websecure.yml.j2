tcp:
  routers:
    websecuretcp:
      entryPoints:
        - "websecuretcp"
      rule: "HostSNI(`*`)"
      service: websecuretcp
      tls:
        passthrough: true

  services:
    websecuretcp:
      loadBalancer:
        proxyProtocol:
          version: 2
        servers:
          - address: "{{ proxy_target_ip }}:443"
