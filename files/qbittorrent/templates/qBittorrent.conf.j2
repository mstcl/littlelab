[General]
ported_to_new_savepath_system=true

[Application]
FileLogger\Age=1
FileLogger\AgeType=1
FileLogger\Backup=true
FileLogger\DeleteOld=true
FileLogger\Enabled=true
FileLogger\MaxSizeBytes=66560
FileLogger\Path=/data/.local/share/qBittorrent/logs

[AutoRun]
OnTorrentAdded\Enabled=true
OnTorrentAdded\Program=/data/utils/add.sh %N
enabled=true
program=/data/utils/finish.sh %N

[BitTorrent]
Session\DefaultSavePath=/data/downloads
Session\DisableAutoTMMByDefault=false
Session\ExcludedFileNames=
Session\Port={{ io_port }}
Session\QueueingSystemEnabled=true

[Core]
AutoDeleteAddedTorrentFile=Never

[LegalNotice]
Accepted=true

[Meta]
MigrationVersion=6

[Network]
PortForwardingEnabled=false
Proxy\HostnameLookupEnabled=false
Proxy\Profiles\BitTorrent=true
Proxy\Profiles\Misc=true
Proxy\Profiles\RSS=true

[Preferences]
Connection\PortRangeMin={{ io_port }}
Downloads\SavePath=/data/downloads
General\Locale=en
MailNotification\password={{ password }}
MailNotification\req_auth=true
WebUI\AuthSubnetWhitelist=@Invalid()
WebUI\Enabled=true
WebUI\Password_PBKDF2="{{ password_encrypted }}"
WebUI\Port=8080
WebUI\RootFolder=/data/utils/vuetorrent
WebUI\Username={{ user }}

[RSS]
AutoDownloader\DownloadRepacks=true
AutoDownloader\SmartEpisodeFilter=s(\\d+)e(\\d+), (\\d+)x(\\d+), "(\\d{4}[.\\-]\\d{1,2}[.\\-]\\d{1,2})", "(\\d{1,2}[.\\-]\\d{1,2}[.\\-]\\d{4})"
